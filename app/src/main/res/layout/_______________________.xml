<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content">


	<View
		android:id="@+id/_bg_____________"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@drawable/_bg______________shape"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageView
		android:id="@+id/imageViewLoader"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:src="@drawable/icon"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginTop="24dp"/>

	<TextView
		android:id="@+id/___________"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Регистрация"
		android:textColor="@color/gray"
		android:textSize="28sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="139dp"/>

	<EditText
		android:id="@+id/phone_number"
		android:layout_width="0dp"
		android:layout_height="37dp"
		android:layout_marginStart="30dp"
		android:layout_marginTop="260dp"
		android:layout_marginEnd="30dp"
		android:background="@drawable/rectangle_13_shape"
		android:inputType="textEmailAddress"
		app:layout_constraintEnd_toEndOf="@id/_bg_____________"
		app:layout_constraintStart_toStartOf="@id/_bg_____________"
		app:layout_constraintTop_toTopOf="@id/_bg_____________" />


	<EditText
		android:id="@+id/usernameedt"
		android:layout_width="0dp"
		android:layout_height="37dp"
		android:layout_marginStart="30dp"
		android:layout_marginTop="325dp"
		android:layout_marginEnd="30dp"
		android:background="@drawable/rectangle_13_shape"
		android:inputType="text"
		app:layout_constraintEnd_toEndOf="@id/_bg_____________"
		app:layout_constraintStart_toStartOf="@id/_bg_____________"
		app:layout_constraintTop_toTopOf="@+id/_bg_____________" />

	<EditText
		android:id="@+id/pswd"
		android:layout_width="0dp"
		android:layout_height="37dp"
		android:layout_marginStart="30dp"
		android:layout_marginTop="390dp"
		android:layout_marginEnd="30dp"
		android:background="@drawable/rectangle_13_shape"
		android:inputType="textPassword"
		app:layout_constraintEnd_toEndOf="@id/_bg_____________"
		app:layout_constraintStart_toStartOf="@id/_bg_____________"
		app:layout_constraintTop_toTopOf="@+id/_bg_____________" />

	<Button
		android:id="@+id/button"
		android:layout_width="0dp"
		android:layout_height="37dp"
		android:layout_marginStart="30dp"
		android:layout_marginEnd="30dp"
		android:layout_marginTop="25dp"
		android:background="@drawable/rectangle_16_shape"
		android:text="Вход"
		android:textColor="@color/main_dark"
		android:textSize="16sp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toBottomOf="@id/pswd" />

	<TextView
		android:id="@+id/_______________"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="35dp"
		android:layout_marginTop="16dp"
		android:text="Элекронная почта:"
		android:textColor="@color/gray"
		android:textSize="11sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.5"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintBottom_toTopOf="@id/phone_number" />

	<TextView
		android:id="@+id/_________________"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="35dp"
		android:layout_marginTop="16dp"
		android:text="Имя пользователя:"
		android:textColor="@color/gray"
		android:textSize="11sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.5"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintBottom_toTopOf="@id/usernameedt" />

	<TextView
		android:id="@+id/_______"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:layout_marginStart="35dp"
		android:layout_marginTop="16dp"
		android:text="Пароль:"
		android:textColor="@color/gray"
		android:textSize="11sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.5"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintBottom_toTopOf="@id/pswd" />


</androidx.constraintlayout.widget.ConstraintLayout>
