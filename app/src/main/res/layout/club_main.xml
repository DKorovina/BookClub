		android:layout_height="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@id/rectangle_1">

		<RelativeLayout android:id="@+id/content_container"
			android:layout_width="match_parent"
			android:layout_height="match_parent">

			<TextView android:id="@+id/_________"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textSize="18sp"
				android:textColor="@color/__________color"
				android:layout_marginLeft="15dp"
				android:layout_marginTop="15dp"
				android:text="Мои клубы"
				/>

			<View
				android:id="@+id/rectangle_21"
				android:layout_width="wrap_content"
				android:layout_height="105dp"
				android:layout_below="@+id/_________"
				android:layout_marginStart="10dp"
				android:layout_marginTop="10dp"
				android:layout_marginEnd="10dp"
				android:layout_marginBottom="10dp"
				android:alpha="0.5"
				android:background="@drawable/rectangle_21_shape" />

			<TextView
				android:id="@+id/______________ek1"
				android:layout_width="wrap_content"
