			<ImageView
				android:id="@+id/rectangle_24"
				android:layout_width="68dp"
				android:layout_height="105dp"
				android:layout_alignStart="@+id/rectangle_21"
				android:layout_alignTop="@+id/rectangle_21"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:contentDescription="rectangle_24"
				android:src="@drawable/rectangle_24" />


		</RelativeLayout>

	</ScrollView>
	<View
		android:id="@+id/rectangle_1"
		android:layout_width="match_parent"
		android:layout_height="68dp"
		android:background="@drawable/rectangle_1_shape"
