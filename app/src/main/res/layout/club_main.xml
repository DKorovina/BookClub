				android:background="@drawable/rectangle_26_shape" />

			<TextView
				android:id="@+id/_________________ek1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignTop="@+id/rectangle_26"
				android:layout_marginStart="15dp"
				android:layout_marginTop="15dp"
				android:layout_toEndOf="@+id/rectangle_27"
				android:text="Русская классика"
				android:textColor="@color/rectangle_2_color"
				android:textSize="18sp" />

			<ImageView
				android:id="@+id/rectangle_27"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignStart="@+id/rectangle_26"
				android:layout_alignTop="@+id/rectangle_26"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:contentDescription="rectangle_27"
				android:src="@drawable/rectangle_27" />

			<ImageView
				android:id="@+id/line_21"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_below="@+id/__________ek2"
				android:layout_marginTop="5dp"
				android:contentDescription="line_21"
				android:src="@drawable/line_21" />

			<View
				android:id="@+id/_bg__group_21_ek1"
				android:layout_width="354dp"
				android:layout_height="105dp"
				android:layout_marginStart="-3dp"
				android:layout_marginTop="-319dp"
