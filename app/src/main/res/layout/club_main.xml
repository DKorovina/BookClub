				android:textColor="@color/rectangle_2_color"
				android:textSize="18sp" />

			<TextView
				android:id="@+id/__________ek1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_below="@+id/rectangle_21"
				android:layout_marginTop="15dp"
				android:text="Все клубы"
				android:textColor="@color/__________ek1_color"
				android:textSize="14sp" />

			<ImageView
				android:id="@+id/line_20"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:layout_below="@+id/__________ek1"
				android:layout_marginTop="5dp"
